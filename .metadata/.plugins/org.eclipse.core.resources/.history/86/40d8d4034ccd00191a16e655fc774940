package com.studyboard.model;

public class MemberDTO {
	String mid;
	String mname;
	String pw;

	public MemberDTO(String mid, String mname, String pw) {
		this.mid = mid;
		this.mname = mname;
		this.pw = pw;
	}
	
	public String getMid() {
		return mid;
	}
	
	public void setMid(String mid) {
		this.mid = mid;
	}
	
	public String getMname() {
		return mname;
	}
	
	public void setMname(String mname) {
		this.mname = mname;
	}
	
	public String getPw() {
		return pw;
	}
	public void setPw(String pw) {
		this.pw = pw;
	}

	
}
