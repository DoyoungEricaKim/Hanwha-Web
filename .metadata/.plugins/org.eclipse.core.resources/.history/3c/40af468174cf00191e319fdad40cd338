    .create( document.querySelector( '#pcontent' ) ) 

    .then( editor => { 

        console.log( editor ); 

    } ) 

    .catch( error => { 

        console.error( error ); 

    } );


config.extraPlugins = 'autogrow';

config.autoGrow_minHeight = 800;

